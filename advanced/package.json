{
  "name": "graphql-boilerplate",
  "scripts": {
    "start": "dotenv -- nodemon -e ts,graphql -x ts-node src/index.ts",
    "debug": "dotenv -- nodemon -e ts,graphql -x ts-node --inspect src/index.ts",
    "build": "rimraf dist && tsc"
  },
  "dependencies": {
    "bcryptjs": "2.4.3",
    "graphql-yoga": "1.17.4",
    "jsonwebtoken": "8.5.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "2.4.2",
    "@types/node": "10.12.27",
    "dotenv-cli": "1.4.0",
    "nodemon": "1.18.10",
    "prisma": "1.27.3",
    "rimraf": "2.6.3",
    "ts-node": "7.0.1",
    "typescript": "3.2.2"
  }
}
