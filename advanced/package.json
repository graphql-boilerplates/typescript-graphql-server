{
  "name": "graphql-boilerplate",
  "scripts": {
    "start": "nodemon -x 'ts-node -r dotenv/config' -e ts,graphql src/index.ts",
    "debug": "nodemon -x 'ts-node -r dotenv/config --inspect' -e ts,graphql src/index.ts",
    "prepare": "graphql prepare",
    "playground": "graphql playground",
    "build": "rimraf dist && tsc",
    "deploy": "now --public --dotenv .env.prod && now alias && now rm --yes --safe graphql-boilerplate",
    "now-start": "node dist"
  },
  "dependencies": {
    "bcryptjs": "2.4.3",
    "graphcool-binding": "1.3.1",
    "graphql-yoga": "1.1.1",
    "jsonwebtoken": "8.1.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "2.4.1",
    "dotenv": "4.0.0",
    "graphql-cli": "2.10.0",
    "nodemon": "1.14.7",
    "now": "9.0.1",
    "rimraf": "2.6.2",
    "ts-node": "4.1.0",
    "typescript": "2.6.2"
  },
  "now": {
    "alias": "graphql-boilerplate"
  }
}
