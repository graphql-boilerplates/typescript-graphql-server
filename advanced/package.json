{
  "name": "graphql-boilerplate",
  "scripts": {
    "start": "dotenv -- nodemon -x ts-node -e ts,graphql src/index.ts",
    "debug":
      "dotenv -- nodemon -x 'ts-node --inspect' -e ts,graphql src/index.ts",
    "playground": "graphql playground",
    "build": "rimraf dist && tsc",
    "deploy":
      "now --public --dotenv .env.prod && now alias && now rm --yes --safe graphql-boilerplate",
    "now-start": "node dist"
  },
  "dependencies": {
    "bcryptjs": "2.4.3",
    "prisma-binding": "1.3.6",
    "graphql-import": "0.4.0",
    "graphql-yoga": "0.4.7",
    "jsonwebtoken": "8.1.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "2.4.1",
    "dotenv-cli": "1.4.0",
    "graphql-cli": "2.12.1",
    "nodemon": "1.14.11",
    "now": "8.5.4",
    "rimraf": "2.6.2",
    "ts-node": "4.1.0",
    "typescript": "2.6.2",
    "prisma-cli": "beta"
  },
  "now": {
    "alias": "graphql-boilerplate"
  }
}
